#include <iostream>
#include <vector>
using namespace std;
int main(){
    for(int i = 1; i < 2147483647; i++){

        int counter = 0;
        int j = 1;
        while(j <= 20){
            if(i % j == 0 ){
                counter++;
            }
            j++;
        }
        if(counter == 20){
            cout << i;
            return 0;
        }
    }
}
